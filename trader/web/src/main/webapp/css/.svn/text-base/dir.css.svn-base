/*****Max*start*block****/
.table-top
{
	background:url(../images/logo.jpg) no-repeat #9daed7;
    font-family: Arial;
}

.table-top-title
{
	vertical-align:middle;
    text-align:center;
    font-weight:bold;
    font-size:12px;
}

.table-top-version
{
    color:white;
    font-size:10px;
    font-weight:bold;
    vertical-align:bottom;
    padding-left:5px;
}
/*****Max*end*block***/

.mainBorder
{
	border-top: 1px solid #cccccc;
}

.itemBorder
{
	padding:2px 2px;
	border-bottom: 1px solid #cccccc;
	border-left: 1px solid #cccccc;
	border-right: 1px solid #cccccc;
}

.item
{
	padding:2px 2px;
	background-color:#f0f3f9;  /*rewritten*/
	cursor:pointer;
}

.itemOver
{
	padding:2px 2px;
	background-color:#EEEEEE;
	color:#4F77A3;
	cursor:pointer;
}

.itemDown
{
	padding:2px 2px;
	background-color:#dee7fd;  /*rewritten*/
	cursor:pointer;
    color:black;               /*rewritten*/
    font-weight:bold;          /*rewritten*/
}

.area-border
{
	padding: 2px 2px;
	border-top: 1px solid #E0E0E0;
	border-left: 1px solid #E0E0E0;
	border-right: 1px solid #E0E0E0;
	border-bottom: 1px solid #E0E0E0;
	background-color: white;                /*rewritten*/
}

.row-title
{
	font-size: 10px;
	font-weight: bold;
    color:white;              /*rewritten*/
    background-color:#586ea0; /*rewritten*/
    padding: 2px 4px;	
	border-top: 0px solid #E0E0E0;
	border-left: 0px solid #E0E0E0;
	border-right: 1px solid #E0E0E0;
	border-bottom: 1px solid #E0E0E0;
}

.row-title-free-new    /*new, see colors*/
{
    font-size: 10px;
    font-weight: bold;
	padding: 2px 4px;
	background-color: #dee7fd;;
	border-top: 0px solid #E0E0E0;
	border-left: 0px solid #E0E0E0;
	border-right: 0px solid #E0E0E0;
	border-bottom: 1px solid #E0E0E0;
}

.row-title-paging    /*new, changed only colors*/
{
	font-size: 10px;
	font-weight: bold;
    color:black;
    background-color:white;
    padding: 2px 4px;
	border-top: 0px solid #E0E0E0;
	border-left: 0px solid #E0E0E0;
	border-right: 1px solid #E0E0E0;
	border-bottom: 1px solid #E0E0E0;    
}

#area_nav
{
    background-color: #dee7fd;
}
.blue_row
{
    background-color: #dee7fd;
}

.row-free
{
    background-color: white;    /*rewrite old background-color*/
}

.money-border
{
    background-color: white;    /*rewrite old background-color*/
}

