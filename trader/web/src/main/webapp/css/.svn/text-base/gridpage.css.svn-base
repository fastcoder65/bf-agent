.marketNotAvailable
{
	position: absolute;
	top: 0;
	left: 0;
	z-index:10;
}

.currentMarket
{
	padding: 0 0;
	text-align: right;
	font-family: tahoma;
	font-size: 11px;
	font-weight: bold;
}

.currentEvent
{
	padding: 0 0;
	text-align: right;
	font-family: tahoma;
	font-size: 11px;
	font-weight: normal;
}

.grid-border-test
{
	overflow:auto;
	padding: 0 0;
	border-top: 2px solid #EEAAEE;
	border-left: 2px solid #EEAAEE;
	border-right: 2px solid #EEAAEE;
	border-bottom: 2px solid #EEAAEE;
}


.grid-border
{
	padding: 0 0;
	border-top: 1px solid #E0E0E0;
	border-left: 1px solid #E0E0E0;
	border-right: 1px solid #E0E0E0;
	border-bottom: 0 solid #E0E0E0;
}

.separator
{
	padding: 0 0;
	background: #FFFFFF;
	border-top: 0 solid #E0E0E0;
	border-left: 0 solid #E0E0E0;
	border-right: 0 solid #E0E0E0;
	border-bottom: 0 solid #E0E0E0;
}


.sizer-left
{
	padding: 0 0;
	text-align: left;
	background: #FFFFFF;
	border-top: 0 solid #E0E0E0;
	border-left: 0 solid #E0E0E0;
	border-right: 1px solid #E0E0E0;
	border-bottom: 0 solid #E0E0E0;
}
.sizer-right
{
	padding: 0 0;
	text-align: left;
	background: #FFFFFF;
	border-top: 0 solid #E0E0E0;
	border-left: 1px solid #E0E0E0;
	border-right: 0 solid #E0E0E0;
	border-bottom: 0 solid #E0E0E0;
}


.title-name
{
	padding: 0 4px;
	text-align: left;
	background: #FFFFFF;
	border-top: 0 solid #E0E0E0;
	border-left: 0 solid #E0E0E0;
	border-right: 0 solid #E0E0E0;
	border-bottom: 1px solid #E0E0E0;
}

.title-back
{
	padding: 0 0;
	background: #FFFFFF;
}
.title-backArrow
{
	padding: 0 0;
	background: #c9dae5;
}
.title-left
{
	padding: 0 0;
	text-align: left;
	background: #FFFFFF;
	border-top: 0 solid #E0E0E0;
	border-left: 0 solid #E0E0E0;
	border-right: 1px solid #E0E0E0;
	border-bottom: 1px solid #E0E0E0;
}
.title-right
{
	padding: 0 0;
	text-align: right;
	background: #FFFFFF;
	border-top: 0 solid #E0E0E0;
	border-left: 1px solid #E0E0E0;
	border-right: 0 solid #E0E0E0;
	border-bottom: 1px solid #E0E0E0;
}
.title-layArrow
{
	padding: 0 0;
	background: #F9F9C9;
}
.title-lay
{
	padding: 0 0;
	background: #FFFFFF;
}




.cell-name
