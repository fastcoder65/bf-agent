.table-top
{
	background:url(../images/logo.jpg) no-repeat #9daed7;
    font-family: Arial;
}

.table-top-title
{
	vertical-align:middle;
    text-align:center;
    font-weight:bold;
    font-size:12px;
}

.table-top-version
{
    color:white;
    font-size:10px;
    font-weight:bold;
    vertical-align:bottom;
    padding-bottom: 3px;
    padding-left:5px;
}

.navigation-btn {
    font-size: 11px;
    font-family: Verdana, Arial, sans-serif;
    background-color: #F0F0F0;
    border-top: 1px solid #C0C0C0;
    border-left: 1px solid #C0C0C0;
    border-right: 1px solid #C0C0C0;
    border-bottom: 1px solid #C0C0C0;
}

.navigation-btn-highlight {
    font-size: 11px;
    font-family: Verdana, Arial, sans-serif;
    background-color: #FFFFFF;
    border-top: 1px solid #C0C0C0;
    border-left: 1px solid #C0C0C0;
    border-right: 1px solid #C0C0C0;
    border-bottom: 1px solid #C0C0C0;
    cursor: pointer;
    cursor: hand;
}