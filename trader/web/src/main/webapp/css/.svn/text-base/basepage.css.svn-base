/*
 * ========================================
 * styles for pagination links
 * ========================================
 */


.pagelink,
.pagelinklast,
.pagecurrent{
        font-family: tahoma, arial;
        font-weight: bold;
        font-size: 11px;
        padding: 1px 3px 1px 3px;
}

.pagedisable{
        font-family: tahoma, arial;
        font-weight: bold;
        color: #999999;
        font-size: 11px;
        padding: 1px 3px 1px 3px;
}

.pagelinklast{
        background: #DFE6EF;
}


.pagecurrent{
        background: #E0E0E0;
}

.pagelink a:active,
.pagelink a:visited,
.pagelink a:link,
.pagelinklast a:active,
.pagelinklast a:visited,
.pagelinklast a:link,
.pagecurrent a:active,
.pagecurrent a:visited,
.pagecurrent a:link{
        font-family: tahoma, arial;
        font-weight: bold;
        font-size: 11px;
        padding: 1px 3px 1px 3px;
}

.spanSelect {
	font-weight: bold;
}
.spanNoSelect {
	font-weight: normal;
}