.topmenu {
    margin:0 50px 0 0;
    color:#fff;
    text-decoration:none;
}

input, input.marketButton, input.marketButtonHover, input.button {
    font-size: 11px;
    font-family: Verdana, Arial, sans-serif;
    border: 1px solid #C0C0C0;
}

input, input.marketButton, input.button  {
    background-color: #F0F0F0;
}

input.marketButtonHover {
    background-color: #fff;
    cursor: pointer;
}


