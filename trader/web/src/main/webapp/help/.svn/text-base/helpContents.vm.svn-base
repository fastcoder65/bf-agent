#parse("/WEB-INF/resources/include/meta/htmlDescriptor.vm")
<html>
<head>
	<title>$betext.get("help.titleContents")</title>
	<link rel='stylesheet' href='$link.setRelative("css/main.css")' type='text/css'>
	<link rel='stylesheet' href='$link.setRelative("css/basepage.css")' type='text/css'>
    <link rel='stylesheet' href='$link.setRelative("css/dir.css")' type='text/css'>
    <link rel='stylesheet' href='$link.setRelative("css/common.css")' type='text/css'>
    <script type="text/javascript">
		function op() { //This function is used with folders that do not open pages themselves. See online docs.
		}
	</script>
</head>

<body>

<table border='0' cellspacing='1' cellpadding='0' width='100%' height='100%' style="background-color:white;">
    	<tr>
		<td align='center' colspan='2' width='100%' height='40px'>
			<table border='0' cellspacing='0' cellpadding='0' width='100%' height='100%' class="table-top">
			<tr>
            <td width='165px'>
                <table border='0' cellspacing='0' cellpadding='0' width='100%' height='100%'>
				<tr>
				<td nowrap width='165px' class="table-top-version" >
                        $betext.get("top.logo1") $betext.get("login.version") $!application.getAttribute("version")
                </td>
				</tr>
				</table>
			</td>
            <td class="table-top-title" style="text-align:right; padding-right:10px;">$betext.get("help.titleContents")</td>
        </tr>
		</table>
	</td>
</tr>

	<tr>
		<td>
			<table cellspacing="1" cellpadding="0" border="0" width='100%' height='100%'>
				<tr>
					<td class='area-border' valign="top" align="left" width='35%'>
						<iframe src='$link.setForward("helpTree")' id="treeHelp" name="treeHelp" scrolling="yes"
										width='100%' height='100%' marginwidth="0" marginheight="0" frameborder="0">
						</iframe>
					</td>
					<td class='area-border' valign="top" align="left">
						<iframe src='$link.setForward("helpPage")' id="main" name="main"
										scrolling="yes" frameborder="0" marginwidth="0" marginheight="0"
										width='100%' height='100%'>
						</iframe>
					</td>
				</tr>
			</table>

		</td>
	</tr>
	<tr>
		<td class='area-border' width='100%' height='30px'>
			<table border='0' cellspacing='2' cellpadding='0' width='100%' height='100%'>
				<tr>
					<td>
						<input type="button"
									 value='$betext.get("help.close")'
									 title='$betext.get("help.close")'
									 onMouseOver='this.className="highlight"'
									 onMouseOut='this.className=""'
									 onClick="window.close();">
					</td>
				</tr>
			</table>
		</td>
	</tr>

</table>
</body>
</html>
